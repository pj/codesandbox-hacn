{
  "compilerOptions": {
    "strict": true,
    "lib": [
      "dom",
      "es2017"
    ],
    "jsx": "react",
    "jsxFactory": "hacn.createElement",
    "target": "es2018",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "rootDir": "src"
  }
}
